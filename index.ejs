<!doctype html>
<html>
    <head>
        <meta charset="utf-8">
        <title>Metaviz</title>
        <meta name="description" content="Metaviz">
        <meta http-equiv="Cache-Control" content="no-cache, no-store, must-revalidate">
        <meta http-equiv="Pragma" content="no-cache">
        <meta http-equiv="Expires" content="0">
        <meta name="viewport" content="width=device-width, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0">
        <meta name="metaviz:agent:name" content="standalone">
        <meta name="metaviz:agent:client" content="">
        <meta name="metaviz:agent:server" content="">
        <meta name="metaviz:agent:data" content="">
        <meta name="metaviz:agent:db" content="">
        <meta name="metaviz:url:data" content="">
        <meta name="metaviz:url:sync" content="">
        <meta name="metaviz:url:upload" content="">
        <meta name="metaviz:url:cmd" content="">
        <link rel="shortcut icon" type="image/svg" href="data:image/svg+xml;base64,">
        <style type="text/css">
            <%- mincss('metaviz.css') %>
            <%- mincss('../metaviz-gui/promenu/promenu.css') %>
            <%- mincss('../metaviz-gui/promenu/promenu-widgets.css') %>
            <%- mincss('../metaviz-gui/promenu/widgets/promenu-option.css') %>
            <%- mincss('../metaviz-gui/promenu/widgets/promenu-input.css') %>
            <%- mincss('../metaviz-gui/promenu/widgets/promenu-separator.css') %>
            <%- mincss('../metaviz-gui/promenu/widgets/promenu-group.css') %>
            <%- mincss('../metaviz-gui/promenu/widgets/promenu-submenu.css') %>
            <%- mincss('../metaviz-gui/promenu/widgets/promenu-select.css') %>
            <%- mincss('../metaviz-gui/promenu/widgets/promenu-switch.css') %>
            <%- mincss('../metaviz-gui/popup/window.css') %>
            <%- mincss('../metaviz-links/bezier.css') %>
            <%- mincss('../metaviz-links/symlink.css') %>
            <%- mincss('../metaviz-nodes/default/node-point.css') %>
            <%- mincss('../metaviz-nodes/default/node-label.css') %>
            <%- mincss('../metaviz-nodes/default/node-text.css') %>
            <%- mincss('../metaviz-nodes/default/node-clipart.css') %>
            <%- mincss('../metaviz-nodes/default/node-vcard.css') %>
        </style>
        <script type="text/javascript">
            let metaviz = null;
            <%- await minjs('utils/logging.js') %>
            <%- await minjs('config.js') %>
            <%- await minjs('runtime.js') %>
            <%- await minjs('metaviz.js') %>
            <%- await minjs('editor/state.js') %>
        </script>
        <script type="text/javascript" src="https://kit.fontawesome.com/6db2155d38.js" crossorigin="anonymous"></script>
    </head>
    <body>
        <div id="metaviz-diagram" class="metaviz-diagram"></div>
        <div id="metaviz-spinner" style="fill-opacity: 1; fill-rule: nonzero; stroke: none">
            <svg xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:svg="http://www.w3.org/2000/svg" xmlns="http://www.w3.org/2000/svg" version="1.1" viewBox="0 0 50 50" height="64" width="64">
                <g transform="translate(0,-247)">
                    <g transform="matrix(0.35277777,0,0,-0.35277777,-42.666529,253.98439)">
                        <g style="fill: var(--metaviz-logo-save);" transform="translate(240.41395,-34.864343)">
                            <path d="M 0,0 -16.2,16.202 -32.404,32.407 -32.405,32.405 -48.601,48.604 -64.806,32.405 -64.808,32.407 -81.005,16.202 -97.213,0 -113.41,-16.2 -97.213,-32.4 -81.005,-16.2 -64.808,0 -48.601,-16.204 -32.404,0 -16.2,-16.2 0.001,-32.405 16.205,-16.204 Z"/>
                        </g>
                        <g style="fill: var(--metaviz-logo-save);" transform="translate(208.00965,-67.269543)">
                            <path d="M 0,0 -16.197,-16.2 -32.403,0 -48.601,-16.2 -32.403,-32.404 -16.197,-48.604 0,-32.404 16.204,-16.2 Z"/>
                        </g>
                    </g>
                </g>
            </svg>
        </div>
        <script type="text/javascript">
            <%- await minjs('utils/jsext.js') %>
            <%- await minjs('utils/events.js') %>
            <%- await minjs('../total-text/render.js') %>
            <%- await minjs('controls/base.js') %>
            <%- await minjs('controls/badge.js') %>
            <%- await minjs('controls/bitmap.js') %>
            <%- await minjs('controls/button.js') %>
            <%- await minjs('controls/icon.js') %>
            <%- await minjs('controls/input.js') %>
            <%- await minjs('controls/label.js') %>
            <%- await minjs('controls/spinner.js') %>
            <%- await minjs('controls/link.js') %>
            <%- await minjs('controls/select.js') %>
            <%- await minjs('controls/slider.js') %>
            <%- await minjs('controls/richtext.js') %>
            <%- await minjs('../metaviz-gui/promenu/promenu-widget.js') %>
            <%- await minjs('../metaviz-gui/promenu/promenu-panel.js') %>
            <%- await minjs('../metaviz-gui/promenu/promenu.js') %>
            <%- await minjs('../metaviz-gui/promenu/promenu-widgets.js') %>
            <%- await minjs('../metaviz-gui/promenu/widgets/promenu-option.js') %>
            <%- await minjs('../metaviz-gui/promenu/widgets/promenu-input.js') %>
            <%- await minjs('../metaviz-gui/promenu/widgets/promenu-separator.js') %>
            <%- await minjs('../metaviz-gui/promenu/widgets/promenu-group.js') %>
            <%- await minjs('../metaviz-gui/promenu/widgets/promenu-submenu.js') %>
            <%- await minjs('../metaviz-gui/promenu/widgets/promenu-select.js') %>
            <%- await minjs('../metaviz-gui/promenu/widgets/promenu-switch.js') %>
            <%- await minjs('../metaviz-gui/popup/window.js') %>
            <%- await minjs('arrange/geometry.js') %>
            <%- await minjs('arrange/sort.js') %>
            <%- await minjs('arrange/align.js') %>
            <%- await minjs('../total-diagram/node.js') %>
            <%- await minjs('../total-diagram/link.js') %>
            <%- await minjs('../total-diagram/nodes.js') %>
            <%- await minjs('../total-diagram/links.js') %>
            <%- await minjs('../total-diagram/render.js') %>
            <%- await minjs('../metaviz-links/bezier.js') %>
            <%- await minjs('../metaviz-links/symlink.js') %>
            <%- await minjs('themes/iron.js') %>
            <%- await minjs('themes/covellite.js') %>
            <%- await minjs('editor/link.js') %>
            <%- await minjs('editor/links.js') %>
            <%- await minjs('editor/socket.js') %>
            <%- await minjs('editor/node.js') %>
            <%- await minjs('editor/nodes.js') %>
            <%- await minjs('editor/navigator.js') %>
            <%- await minjs('editor/cage.js') %>
            <%- await minjs('editor/keyboard.js') %>
            <%- await minjs('editor/pointer.js') %>
            <%- await minjs('editor/menu.js') %>
            <%- await minjs('editor/selection.js') %>
            <%- await minjs('editor/editor.js') %>
            <%- await minjs('format/stack/data-in.js') %>
            <%- await minjs('format/stack/data-out.js') %>
            <%- await minjs('storage/filesystem/data-in-out.js') %>
            <%- await minjs('storage/indexeddb/data-in-out.js') %>
            <%- await minjs('../metaviz-nodes/default/node-point.js') %>
            <%- await minjs('../metaviz-nodes/default/node-label.js') %>
            <%- await minjs('../metaviz-nodes/default/node-text.js') %>
            <%- await minjs('../metaviz-nodes/default/node-clipart.js') %>
            <%- await minjs('../metaviz-nodes/default/node-vcard.js') %>
            window.addEventListener('load', function() {
                metaviz = new Metaviz();
                if (metaviz.compatibilityTest()) {
                    metaviz.init('metaviz-diagram', 'metaviz-spinner');
                    metaviz.start();
                }
                else {
                    alert('Unsupported browser version. Please update to the latest one.');
                }
            });
        </script>
    </body>
</html>
